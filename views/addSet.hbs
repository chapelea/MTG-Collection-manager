<div class="container">
  <h1>Add Set</h1>
  {{#each errors}}
  <div class="alert alert-danger">
    {{ this }}
  </div>
  {{/each}}
  <form method="post">
    <div class="form-group">
      <label for="set_name">Set Name</label>
      <input
        class="form-control"
        id="set_name"
        type="name"
        name="set_name"
      />
    </div>
    <div class="form-group">
      <label for="set_code">Set Code</label>
      <input
        class="form-control"
        id="set_code"
        type="name"
        name="set_code"
      />
    </div>
    <button class="btn btn-primary" type="submit">Submit</button>
  </form>
  <a href="/">Cancel</a>
  
</div>